{
  "Logging": {
    "Region": "us-east-1",
    "LogGroup": "/aws/lanes-backend/prod",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ClassroomGroups": {
    "AppSecrets": {
      "SystemsManagerPath": "/ClassroomGroups/Prod/"
    },
    "ConnectionString": "Data Source=classroom_groups_prod_database.db",
    "LoggedInRedirectUrl": "http://classroomgroups.com",
    "LoggedOutRedirectUrl": "http://classroomgroups.com"
  },
  "AllowedHosts": "*"
}
